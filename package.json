{
	"name": "todot",
	"version": "1.0.0",
	"description": "A simple TO-DO",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"app:dir": "electron-builder --dir",
		"app:dist": "electron-builder"
	},
	"repository": "https://github.com/electron/electron-quick-start",
	"keywords": [
		"Electron",
		"quick",
		"start",
		"tutorial",
		"demo"
	],
	"author": "Chuaschinai",
	"license": "CC0-1.0",
	"devDependencies": {
		"electron": "^17.0.1",
		"electron-packager": "^15.4.0",
		"electron-builder": "^22.14.13"
	},
	"dependencies": {
		"@electron/remote": "^2.0.5",
		"crel": "^4.2.1",
		"jQuery": "^1.7.4"
	},
	"build": {
		"productName": "todot",
		"appId": "chuaschinai.todot",
		"win": {
			"target": [
				"portable"
			]
		},
		"portable": {
			"artifactName": "todot.exe"
		},
		"directories": {
			"output": "./release",
			"app": "./"
		}
	}
}